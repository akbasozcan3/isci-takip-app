{
  "name": "bavaxe",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:backend": "npm --prefix backend start",
    "start:dev": "npm --prefix backend run dev",
    "start:all": "concurrently -n backend,app -c cyan,magenta \"npm --prefix backend run dev\" \"expo start\"",
    "start:app:lan": "set EXPO_PUBLIC_API_BASE=http://192.168.1.114:4000&& expo start",
    "start:all:lan": "set EXPO_PUBLIC_API_BASE=http://192.168.1.114:4000&& concurrently -n backend,app -c cyan,magenta \"npm --prefix backend run dev\" \"expo start\"",
    "dev": "npm run start:backend",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "npx expo export -p web",
    "build:vercel": "echo 'Vercel build ready'",
    "lint": "expo lint",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "server": "cd backend && npm run start",
    "server:pm2": "cd backend && npm run start",
    "server:stop": "cd backend && npm run stop",
    "server:restart": "cd backend && npm run restart",
    "server:logs": "cd backend && npm run logs",
    "server:status": "cd backend && npm run status"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "expo": "54.0.22",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-task-manager": "~14.0.8",
    "expo-web-browser": "~15.0.9",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
